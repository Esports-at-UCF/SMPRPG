{
    "model": {
        "type": "select",
        "property": "custom_model_data",
        "fallback": {
            "type": "model",
            "model": "item/iron_shovel"
        },
        "cases": [
            {
                "when": "smprpg:silver_shovel",
                "model": {
                    "type": "model",
                    "model": "tools:item/silver_shovel"
                }
            },
            {
                "when": "smprpg:steel_shovel",
                "model": {
                    "type": "model",
                    "model": "tools:item/steel_shovel"
                }
            },
            {
                "when": "smprpg:tin_shovel",
                "model": {
                    "type": "model",
                    "model": "tools:item/tin_shovel"
                }
            }
        ]
    }
}
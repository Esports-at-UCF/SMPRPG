{
    "model": {
        "type": "select",
        "property": "custom_model_data",
        "fallback": {
            "type": "model",
            "model": "item/golden_axe"
        },
        "cases": [
            {
                "when": "smprpg:gold_hatchet",
                "model": {
                    "type": "model",
                    "model": "tools:item/golden_hatchet"
                }
            },
            {
                "when": "smprpg:rose_gold_axe",
                "model": {
                    "type": "model",
                    "model": "tools:item/rose_gold_axe"
                }
            },
            {
                "when": "smprpg:rose_gold_hatchet",
                "model": {
                    "type": "model",
                    "model": "tools:item/rose_gold_hatchet"
                }
            }
        ]
    }
}
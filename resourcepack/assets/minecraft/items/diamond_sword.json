{
    "model": {
        "type": "select",
        "property": "custom_model_data",
        "fallback": {
            "type": "model",
            "model": "item/diamond_sword"
        },
        "cases": [
            {
                "when": "smprpg:elderflame_dagger",
                "model": {
                    "type": "model",
                    "model": "swords:item/elderflame_dagger"
                }
            },
            {
                "when": "smprpg:mithril_sword",
                "model": {
                    "type": "model",
                    "model": "swords:item/mithril_sword"
                }
            },
            {
                "when": "smprpg:titanium_sword",
                "model": {
                    "type": "model",
                    "model": "swords:item/titanium_sword"
                }
            },
            {
                "when": "smprpg:adamantium_sword",
                "model": {
                    "type": "model",
                    "model": "swords:item/adamantium_sword"
                }
            }
        ]
    }
}
{
    "model": {
        "type": "select",
        "property": "custom_model_data",
        "fallback": {
            "type": "model",
            "model": "item/iron_hoe"
        },
        "cases": [
			{
                "when": "smprpg:silver_hoe",
                "model": {
                    "type": "model",
                    "model": "tools:item/silver_hoe"
                }
            },
            {
                "when": "smprpg:steel_hoe",
                "model": {
                    "type": "model",
                    "model": "tools:item/steel_hoe"
                }
            }
        ]
    }
}
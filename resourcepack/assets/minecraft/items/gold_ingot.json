{
    "model": {
        "type": "select",
        "property": "custom_model_data",
        "fallback": {
            "type": "model",
            "model": "item/gold_ingot"
        },
        "cases": [
            {
                "when": "smprpg:boiling_ingot",
                "model": {
                    "type": "model",
                    "model": "materials:item/boiling_ingot"
                }
            },
            {
                "when": "smprpg:rose_gold_ingot",
                "model": {
                    "type": "model",
                    "model": "materials:item/rose_gold_ingot"
                }
            }
        ]
    }
}
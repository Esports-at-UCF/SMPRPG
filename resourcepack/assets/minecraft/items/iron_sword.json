{
    "model": {
        "type": "select",
        "property": "custom_model_data",
        "fallback": {
            "type": "model",
            "model": "item/iron_sword"
        },
        "cases": [
            {
                "when": "smprpg:silver_sword",
                "model": {
                    "type": "model",
                    "model": "swords:item/silver_sword"
                }
            },
            {
                "when": "smprpg:mystbloom_kunai",
                "model": {
                    "type": "model",
                    "model": "swords:item/mystbloom_kunai"
                }
            },
            {
                "when": "smprpg:steel_sword",
                "model": {
                    "type": "model",
                    "model": "swords:item/steel_sword"
                }
            },
            {
                "when": "smprpg:tin_sword",
                "model": {
                    "type": "model",
                    "model": "swords:item/tin_sword"
                }
            }
        ]
    }
}
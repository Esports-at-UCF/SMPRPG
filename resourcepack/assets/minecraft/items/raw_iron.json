{
    "model": {
        "type": "select",
        "property": "custom_model_data",
        "fallback": {
            "type": "model",
            "model": "item/raw_iron"
        },
        "cases": [
            {
                "when": "smprpg:raw_silver",
                "model": {
                    "type": "model",
                    "model": "materials:item/raw_silver"
                }
            },
            {
                "when": "smprpg:raw_tin",
                "model": {
                    "type": "model",
                    "model": "materials:item/raw_tin"
                }
            },
			            {
                "when": "smprpg:raw_mithril",
                "model": {
                    "type": "model",
                    "model": "materials:item/raw_mithril"
                }
            },
			{
                "when": "smprpg:raw_titanium",
                "model": {
                    "type": "model",
                    "model": "materials:item/raw_titanium"
                }
            },
			{
                "when": "smprpg:raw_adamantium",
                "model": {
                    "type": "model",
                    "model": "materials:item/raw_adamantium"
                }
            }
        ]
    }
}